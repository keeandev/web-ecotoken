{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "db:generate": {
            "inputs": ["prisma/schema.prisma"],
            "cache": false
        },
        "db:push": {
            "inputs": ["prisma/schema.prisma"],
            "cache": false
        },
        "db:seed": {
            "inputs": ["prisma/schema.prisma"],
            "cache": false
        },
        "db:studio": {
            "inputs": ["prisma/schema.prisma"],
            "cache": false
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "build": {
            "dependsOn": ["^build", "^db:generate"],
            "outputs": [
                ".next/**",
                ".expo/**",
                "storybook-static/**",
                "dist/**"
            ]
        },
        "lint": {},
        "lint:fix": {},
        "clean": {
            "cache": false
        },
        "//#clean": {
            "cache": false
        },
        "type-check": {
            "dependsOn": ["^db:generate"],
            "cache": false
        }
    },
    "globalEnv": [
        "DATABASE_URL",
        "VERCEL",
        "VERCEL_URL",
        "PORT",
        "NODE_ENV",
        "NEXT_PUBLIC_CDN_URL",
        "NEXT_PUBLIC_SOLANA_RPC",
        "NEXT_PUBLIC_SOLANA_NETWORK",
        "NEXT_PUBLIC_SOLANA_ADMIN_PUBKEY",
        "NEXT_PUBLIC_SOLANA_USDC",
        "SOLANA_ADMIN_WALLET",
        "REGEN_WALLET",
        "REGEN_ENDPOINT",
        "COLLECTION_ADDRESS",
        "EXTERNAL_URL",
        "SPACES_BUCKET",
        "SPACES_KEY",
        "SPACES_SECRET",
        "EMAIL_VERIFICATION_EXPIRE_TIME",
        "EMAIL_VERIFICATION_EMAIL_ADDRESS",
        "EMAIL_CLIENT_ID",
        "EMAIL_PRIVATE_KEY",
        "IRON_SESSION_PASSWORD",
        "IRON_SESSION_EXPIRE_TIME",
        ""
    ]
}
