{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": ["config:base"],
    "osvVulnerabilityAlerts": true,
    "prHourlyLimit": 10,
    "updateInternalDeps": true,
    "postUpdateOptions": ["npmDedupe"],
    "rangeStrategy": "bump",
    "packageRules": [
        {
            "matchPackagePatterns": ["^@ecotoken/"],
            "enabled": false
        }
    ],
    "assignees": ["hjozwiak"]
}
